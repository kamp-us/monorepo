{
  "extends": "@kampus/tsconfig/nextjs.json",
  "compilerOptions": {
    "paths": {
      "basePath": ".",
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
