    "znv": "0.3.2",
    "zod": "3.20.6"
  },
  "devDependencies": {
    "@kampus/eslint-config": "*",
    "@kampus/tsconfig": "*",
    "@remix-run/dev": "1.12.0",
    "@types/eslint": "8.4.5",
    "@types/node": "18.11.19",
    "@types/nprogress": "0.2.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
