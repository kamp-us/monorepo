{
  "name": "@kampus-apps/pano",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint src",
    "lint:fix": "npm run lint -- --fix",
    "dev": "run-p dev:*",
    "dev:remix": "remix dev",
    "dev:prisma": "prisma migrate deploy && prisma generate && prisma db seed",
    "build": "remix build",
    "start": "NODE_ENV=production remix-serve ./build",
    "docker:start": "NODE_ENV=production run-p \"prisma migrate deploy\" start"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "volta": {
    "node": "18.14.2"
  },
  "eslintIgnore": [
    "/node_modules",
    "/server/index.js",
    "/public/build",
    "/build"
  ],
  "dependencies": {
    "@kampus/ui": "*",
    "@prisma/client": "4.9.0",
    "@radix-ui/react-icons": "1.1.1",
    "@remix-run/node": "1.12.0",
    "@remix-run/react": "1.12.0",
    "@remix-run/serve": "1.12.0",
    "bcryptjs": "2.4.3",
    "envsafe": "2.0.3",
    "html-metadata-parser": "2.0.4",
    "normalize-url": "8.0.0",
    "nprogress": "0.2.0",
    "react": "18.2.0",
    "react-copy-to-clipboard": "5.1.0",
    "react-dom": "18.2.0",
    "slugify": "1.6.5",
    "tiny-invariant": "1.3.1"
  },
  "devDependencies": {
    "@kampus/eslint-config": "0.0.0",
    "@kampus/tsconfig": "0.0.0",
    "@remix-run/dev": "1.12.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/bcryptjs": "2.4.2",
    "@types/eslint": "8.21.0",
    "@types/jest": "29.4.0",
    "@types/node": "18.11.19",
    "@types/nprogress": "0.2.0",
    "@types/react": "18.0.27",
    "@types/react-copy-to-clipboard": "5.0.4",
    "@types/react-dom": "18.0.10",
    "cross-env": "7.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.3",
    "prisma": "4.9.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
