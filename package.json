    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^1.0.0",
    "@radix-ui/react-icons": "^1.1.0",
    "@radix-ui/react-label": "^1.0.0",
    "@radix-ui/react-toast": "^1.1.1",
    "@remix-run/node": "1.6.4",
    "@remix-run/react": "1.6.4",
    "@remix-run/serve": "1.6.4",
    "@stitches/react": "^1.2.7",
    "bcryptjs": "^2.4.3",
    "normalize-url": "^7.0.3",
    "nprogress": "^0.2.0",
    "react": "^18.0.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.2.2",
    "react-share": "^4.4.1",
    "slugify": "^1.6.5",
    "tiny-invariant": "^1.2.0"
  },
  "scripts": {
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint src",
    "lint:fix": "npm run lint -- --fix",
    "build": "remix build",
    "external:dev": "cross-env NODE_ENV=development DATABASE_URL=postgresql://pgtest:pgtest@localhost:5432/pgtest?schema=public SESSION_SECRET=sessionsecret npm run migrate:start:dev",
    "prisma:seed": "ts-node ./prisma/seed.ts",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "migrate:start:dev": "npm run prisma:migrate && npm run prisma:seed && remix dev",
    "prepare": "husky install",
    "dev:remix": "remix dev",
    "docker:prisma": "run-s prisma:migrate prisma:seed",
    "docker": "run-p docker:prisma dev:remix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintIgnore": [
    "/node_modules",
    "/server/index.js",
    "/public/build",
    "/build"
  ],
  "devDependencies": {
    "@remix-run/dev": "1.6.4",
    "@remix-run/eslint-config": "1.6.4",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "8.4.5",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.43",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^17.0.14",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "prism": "^4.1.2",
    "prisma": "^4.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3"
  }
